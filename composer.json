{
    "name": "webeweb/xmltv-library",
    "type": "library",
    "description": "Integrate XMLTV with your projects",
    "keywords": [
        "library",
        "webeweb",
        "xmltv"
    ],
    "homepage": "https://github.com/webeweb/xmltv-library",
    "license": "MIT",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "ext-dom": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0",
        "psr/log": "^1.0",
        "webeweb/core-library": "^7.0",
        "webeweb/traits-library": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "WBW\\Library\\XMLTV\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Library\\XMLTV\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/webeweb/xmltv-library/issues"
    }
}
